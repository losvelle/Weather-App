placeholder=\"Search...\"\n            onChange={e => setQuery(e.target.value)}\n            value={query}\n            onKeyPress={search}\n            />\n        </div>\n            {(typeof weather.main != \"undefined\") ? (\n            <div>\n              <div className=\"location-box\">\n                <div className=\"location\">{weather.name}, {weather.sys.country}</div>\n                <div className=\"date\">{dateBuilder(new Date())}</div>\n              </div>\n              <div className=\"weather-box\">\n                <div className=\"temp\">\n                  {Math.round(weather.main.temp)}Â°c\n\n                </div>\n                <div className=\"weather\">{weather.weather[0].main}</div>\n              </div>\n            </div>\n            ) : ('')}\n          </main>\n        </div>\n      );\n    }\n\nexport default App;\n","/Users/admin/Documents/PerScholas/Software Engineering/React Complete Guide/weather-app/weather-react/src/reportWebVitals.js",[],{"ruleId":"29","replacedBy":"30"},{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","severity":1,"message":"34","line":12,"column":1,"nodeType":"35","messageId":"36","endLine":12,"endColumn":69},{"ruleId":"33","severity":1,"message":"34","line":15,"column":5,"nodeType":"35","messageId":"36","endLine":15,"endColumn":77},{"ruleId":"33","severity":1,"message":"34","line":17,"column":7,"nodeType":"35","messageId":"36","endLine":17,"endColumn":167},{"ruleId":"33","severity":1,"message":"34","line":31,"column":5,"nodeType":"35","messageId":"36","endLine":31,"endColumn":153},{"ruleId":"33","severity":1,"message":"34","line":33,"column":9,"nodeType":"35","messageId":"36","endLine":33,"endColumn":91},{"ruleId":"33","severity":1,"message":"34","line":35,"column":9,"nodeType":"35","messageId":"36","endLine":35,"endColumn":86},{"ruleId":"33","severity":1,"message":"34","line":38,"column":9,"nodeType":"35","messageId":"36","endLine":38,"endColumn":113},{"ruleId":"33","severity":1,"message":"34","line"